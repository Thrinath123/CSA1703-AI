print("Enter Row and Column Size of First Matrix: ", end="")
a = int(input())
b = int(input())
print("Enter Row and Column Size of Second Matrix: ", end="")
c= int(input())
d= int(input())
if a==c and b==d:
    matOne = []
    print("\nEnter  Elements for First Matrix: ")
    for i in range(a):
        matOne.append([])
        for j in range(b):
            num = int(input())
            matOne[i].append(num)
    matTwo = []
    print("\nEnter  Elements for Second Matrix: ")
    for i in range(c):
        matTwo.append([])
        for j in range(d):
            num = int(input())
            matTwo[i].append(num)
    matThree = []
    for i in range(a):
        matThree.append([])
        for j in range(d):
            matThree[i].append(matOne[i][j]+matTwo[i][j])
    print("\nAddition Result:")
    for i in range(a):
        for j in range(b):
            print(matThree[i][j], end=" ")
        print()
else:
    print("\nDimension Mismatched!")
